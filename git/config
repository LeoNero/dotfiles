	name = Leonardo Nerone
[core]
	editor = nvim
	excludesfile = /Users/nerone/.config/git/.gitignore_global
	autocrlf = input
[commit]
	template = /Users/nerone/.config/git/.gitmessage.txt
[fetch]
	prune = true
[init]
	defaultBranch = main
[alias]
	cm = commit
	i = init
	cl = clone
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
